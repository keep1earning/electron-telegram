{
  "name": "telegram",
  "version": "1.0.0",
  "description": "Telegram Desktop",
  "main": "index.js",
  "scripts": {
    "debug": "npx electron .",
    "build:windows-x64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.ico --platform=win32 --arch=x64 --overwrite"
    "build:windows-arm64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.ico --platform=win32 --arch=arm64 --overwrite"
    "build:darwin-x64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.icns --platform=darwin --arch=x64 --overwrite"
    "build:darwin-arm64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.icns --platform=darwin --arch=arm64 --overwrite"
    "build:darwin-universal": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.icns --platform=darwin --arch=universal --overwrite"
    "build:linux-x64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.ico --platform=linux --arch=x64 --overwrite"
    "build:linux-arm64": "npx electron-packager ./ Telegram --out ./outputs --asar --icon=./icon.ico --platform=linux --arch=arm64 --overwrite"
  },
  "author": "Hailey",
  "license": "MIT",
  "devDependencies": {
    "electron": "^20.1.0",
    "electron-packager": "^16.0.0"
  }
}
